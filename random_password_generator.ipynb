        "import string\n",
        "import random\n",
        "\n",
        "characters=string.ascii_letters+string.digits+string.punctuation\n",
        "length=int(input(\"Enter the length of the password: \"))\n",
        "password=''.join(random.choices(characters,k=length))\n",
        "print(\"Your password is:\",password)"
